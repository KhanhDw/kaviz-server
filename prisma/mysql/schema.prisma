generator client {
  provider = "prisma-client-js"
  output   = "../generated/mysql-client"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_DATABASE_URL")
}

model User {
  id           String   @id @default(cuid())
  name         String
  email        String   @unique
  createdAt    DateTime @default(now())
}
